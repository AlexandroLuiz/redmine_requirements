<%=render :partial => "requirements/nav"%>


<div id="conteudo">
<div class="contextual"><%=link_to_if_authorized(t(:view_all_categories), {:controller => :categories, :action => :index}, :accesskey => accesskey(:index)) %></div>

<h1><%=l 'edit_category'%></h1>

<% labelled_tabular_form_for :category, @category, :url => { :controller => 'categories', :action => 'update', :project => @project }, :html => {:multipart => true, :remote => true} do |f| %>


<div id="form">
<%= render :partial => "categories/form" , :locals => {:f => f} %>
<%= submit_tag l(:button_update) %>
<% end %>

</div>
</div>

